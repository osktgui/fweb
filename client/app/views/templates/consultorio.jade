include ./mixins/mixins-form.jade
include ./includes/header-int.jade
.body-container-filium.with-left-menu(ng-init="formularioActivo='EditarCV'")
	include ./includes/left-menu.jade
	.formularioActivo(ng-show="formularioActivo=='EditarCV'", ng-controller="EdicioncvCtrl")
		include ./form_edicionCurriculum.jade
		include ./popups/popup-edicionCurriculum.jade
	.formularioActivo(ng-show="formularioActivo=='RegistrarPsicologo'")
		include ./form_registroPsicologos.jade
	.formularioActivo(ng-show="formularioActivo=='CalendarioCitas'", ng-controller="PsicologoCitaCtrl")
		include ./form_psicologoCita.jade
		include ./popups/popup-psicologo-cita.jade


	